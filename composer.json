{
  "name": "webappid/laravel-country",
  "description": "Laravel country package",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "WebAppId\\Country\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebAppId\\Country\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php" : ">=7.2.0",
    "illuminate/support": "^6.0",
    "doctrine/dbal": "^2.5",
    "webappid/laravel-ddd": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "^8.0",
    "mockery/mockery": "dev-master@dev",
    "orchestra/testbench": "^4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "WebAppId\\Country\\ServiceProvider"
      ]
    }
  }
}
